{
  "name": "@tetarchus/google-maps-react",
  "version": "2.1.10",
  "description": "Isomorphic component that allows rendering react components on a google map",
  "author": "tetarchus https://github.com/tetarchus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tetarchus/google-maps-react.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl -f cjs,umd src/index.cjs.js && microbundle-crl -f es,modern src/index.js",
    "start": "microbundle-crl watch -f cjs,umd",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.7.0",
    "@mapbox/point-geometry": "^0.1.0",
    "eventemitter3": "^4.0.4",
    "prop-types": "^15.8.1"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-standard": "^5.0.0",
    "expect": "28.1.1",
    "gh-pages": "^4.0.0",
    "jsdom": "^19.0.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.0",
    "react": "^18.2.0",
    "react-dom": "^16.0.0 || ^17.0.2 || ^18.0.0",
    "react-scripts": "^5.0.1"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "reactjs",
    "google",
    "map",
    "maps",
    "isomorphic",
    "render",
    "component",
    "javascript",
    "react-component"
  ]
}
